<div id="<%= dom_id group %>">
  <p>
    <strong>Nazwa:</strong>
    <%= group.nazwa %>
  </p>

  <p>
    <strong>Semestr:</strong>
    <%= group.semestr %>
  </p>

  <p>
    <% group.students.each do |s| %>
      <%= s.indeks %> <%= s.imie %> <%= s.nazwisko %><br>
    <% end %>
  </p>

  <p>
    <% group.courses.each do |c| %>
      <%= c.nazwa %>
    <% end %>
  </p>

</div>
